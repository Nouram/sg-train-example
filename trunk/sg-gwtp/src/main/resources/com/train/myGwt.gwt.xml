<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='myGwt'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.smartgwt.SmartGwt' />
  <inherits name="com.smartclient.theme.enterprise.Enterprise"/>
    <inherits name="com.allen_sauer.gwt.log.gwt-log-INFO"/>

    <!--<inherits name="com.claudiushauptmann.gwt.multipage.gwt-multipage"/>-->
    <inherits name="org.gwtmultipage.gwtmultipage"/>

    <extend-property name="locale" values="zh"/>

  <!--<entry-point class='com.train.client.myGwt'/>-->
  <entry-point class='org.gwtmultipage.client.EntrypointDispatcher'/>

  <source path='client'/>
  <source path='shared'/>

</module>
